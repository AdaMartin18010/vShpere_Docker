{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://127.0.0.1"
    },
    {
      "pattern": "^#"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "urls": [],
      "headers": {
        "Accept": "text/html",
        "User-Agent": "Mozilla/5.0"
      }
